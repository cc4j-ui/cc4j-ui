        "lib": "npm run dist:style && vue-cli-service build --target lib --name cc4j --dest dist/lib packages/index.js",
        "style": "npm run dist:style",
        "dist:style": "gulp --gulpfile build/build-style.js"
    },
    "dependencies": {
