{
    "name": "cc4j-ui",
    "version": "0.0.1",
    "description": "轻量、规范化、 UI组件库",
    "keywords": [
        "ui组件库",
        "前端组件库"
    ],
    "author": "https://github.com/cc4j-ui",
    "main": "packages/index.js",
    "license": "MIT",
    "private": true,
    "directories": {
        "doc": "doc",
        "examples": "examples",
        "test": "test"
    },
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build --report-json",
