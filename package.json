        "@vue/cli-service": "^3.5.0",
        "babel-eslint": "^10.0.1",
        "babel-plugin-dynamic-import-node": "^2.2.0",
        "eslint": "^5.8.0",
        "eslint-config-standard": "^12.0.0",
        "eslint-plugin-vue": "^5.0.0",
        "gitbook-cli": "^2.3.2",
        "gulp": "^3.9.1",
        "gulp-autoprefixer": "^6.0.0",
        "gulp-clean-css": "^4.0.0",
        "gulp-less": "^4.0.1",
        "gulp-minify-css": "^1.2.4",
        "gulp-rename": "^1.4.0",
        "less": "^3.0.4",
